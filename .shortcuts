# bash profile
alias bpe="vi ~/.bash_profile"
alias bps="source ~/.bash_profile"
alias bpc="cat ~/.bash_profile"

# git
alias gp="git pull"
alias gcs="git checkout staging"
alias gcm="git checkout master"
alias gc="git checkout"
alias ga="git add"
alias gaa="git add ."
alias gl="git log --oneline"
alias glm='git log --oneline --author="Kristina Shapotina"'
alias gb="git branch"
alias gbc="git rev-parse --abbrev-ref HEAD"
alias gst="git stash"
alias gd="git diff"
alias gs="git status"
alias grs="git fetch && git checkout staging && git reset origin/staging --hard"
alias gcom="git commit -m "
alias gpsh="git push"

# sbt
alias new_scala_proj="bash new_scala_proj"
alias sc="sbt compile"
alias st="sbt test"
function sto { sbt "test:testOnly *$1"; }
export -f sto
